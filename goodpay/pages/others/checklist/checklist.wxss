/* pages/checklist/checklist.wxss */

.head{
  width: 680rpx;
  height: 80rpx;
  margin: auto;
  border: 2rpx solid #9f9f9f;
  margin-top: 45rpx;
  border-radius: 80rpx;
  display: flex;
}
.image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 15rpx;
  margin-left: 35rpx;
  float: left;
}

.input{
  width: 570rpx;
  height: 80rpx;
  margin-left: 25rpx;
  float: left;
  color: #585858;
  font-size: 33rpx;
}
.text{
  width: 610rpx;
  height: 50rpx;
  font-size: 26rpx;
  margin: auto;
  line-height: 50rpx;
  color: #adadad;
}

.foot{
  width: 750rpx;
  margin: auto;
  margin-top: 40rpx;
}
.history{
  width:750rpx;
  height: 150rpx;
  margin-top: 35rpx;
  margin: auto;
}
.record{
  width: 680rpx;
  height: 60rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  line-height: 60rpx;
  font-size: 30rpx;
}
.chu{
  color: #4aa7fd;
}
.history-content{
  width: 720rpx;
  margin: auto;
}
.content{
  width: 210rpx;
  height: 60rpx;
  border: 3rpx solid #9f9f9f;
  border-radius: 15rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #383838;
  background: #fff;
  float: left;
  margin: 20rpx 0 0 20rpx;
   
}
.keywords{
  width: 680rpx;
  height: 45rpx;
  margin: auto;
  margin-top: 40rpx;
  line-height: 45rpx;
  font-size: 28rpx;
  color: #4aa7fd;
  clear: both;
}
.buto{
  width: 730rpx;
  margin: auto;
 float: left;
