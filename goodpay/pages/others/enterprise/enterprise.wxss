/* pages/enterprise/enterprise.wxss */
.list{
  width: 750rpx;
  margin-top: 10rpx;
}
.list-name{
  width: 680rpx;
  height: 200rpx;
  padding: 0 36rpx;
  border-bottom: 3rpx solid #f4f2f2;
  font-size: 28rpx;
  color: #585858;
  display: flex;
}
.log{
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
  margin-top: 30rpx;
  border-radius: 10rpx;
}
.log image{
  width: 100%;
  height: 100%;
}
.headline{
  width: 75%;
  height: 140rpx;
  float: left;
  margin-top: 30rpx;
  margin-left: 45rpx;
}
.text{
  width: 100%;
  height: 70rpx;
  text-align: left;
  color: #585858;
  font-size: 28rpx;
}
.text-name{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 26rpx;
  color: #9f9f9f;
}
.text-male{
  margin-left: 30rpx;
}
.text-woma{
  margin-left: 15rpx;
}
.data{
  margin-left: 50rpx;
}
.lode{
  text-align: center;
  font-size: 28rpx;
  color: #9f9f9f;
  line-height: 100rpx;
}