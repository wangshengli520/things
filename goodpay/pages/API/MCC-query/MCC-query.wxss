/* pages/MCC-query/MCC-query.wxss */
.title{
  width: 750rpx;
  height: 130rpx;
  text-align: center;
  line-height: 130rpx;
  font-size: 34rpx;
  color: #383838;
}
.head{
  width: 680rpx;
  height: 65rpx;
  border-bottom: 3rpx solid #585858;
  margin: auto;
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
}
.image{
  width: 45rpx;
  height: 45rpx;
  margin-top: 10rpx;
  float: left;
}
.input{
  width: 610rpx;
  height: 65rpx;
  color: #585858;
  font-size: 30rpx;
  margin-left: 20rpx;
  float: left
}
.button{
  width: 680rpx;
  height: 80rpx;
  margin: auto;
  margin-top: 60rpx;
  color: #fff;
  font-size: 34rpx;
  background: #374256;
  border-radius:10rpx;
  line-height: 80rpx;
  padding: 0;
}
.foot{
  width: 620rpx;
  margin:0 auto;
  padding: 30rpx;
  margin-top: 65rpx;
  border: 3rpx solid #cecece;
  border-radius: 15rpx;
  margin-bottom: 30rpx;
}
.text{
  width: 615rpx;
  margin: auto;
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #383838;
  line-height: 55rpx;
}
















