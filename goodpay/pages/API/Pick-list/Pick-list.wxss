/* pages/Pick-list/Pick-list.wxss */
.header{
  height: 122rpx;
  width: 750rpx;
  background: #ecebeb;
  position: fixed;
  top: 0;
  left: 0;
}
.information{
  width: 730rpx;
  padding: 0 10rpx;
  height: 60rpx;
  border-bottom: 1rpx solid #efe1e3;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #a79b9b;
}
.header-title{
  width: 730rpx;
  padding: 0 10rpx;
  height: 60rpx;
  border-bottom: 1rpx solid #efe1e3;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
  display: flex;
}
.left{
  height: 100%;
  width: 30%;
}
.right{
  height: 100%;
  width: 70%;
  text-indent: 40rpx;
}
.list{
  width: 750rpx;
  margin-top: 130rpx;
}
.info-content{
  width: 730rpx;
  padding: 0 10rpx;
  min-height: 60rpx;
  border-bottom: 1rpx solid #efe1e3;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #333;
  display: flex;
}
.num{
  height: 100%;
  width: 30%;
}
.name{
  height: 100%;
  width: 70%;
}
.zanwu{
  width: 750rpx;
  font-size: 36rpx;
  color: #a79b9b;
  line-height: 100rpx;
  text-align: center;
}
.lode{
  text-align: center;
  font-size: 28rpx;
  color: #9f9f9f;
  line-height: 100rpx;
}